//สร้างClass Employee

public class Employee {
    private String id;
    private String name;
    private int age;
    private double salary; 
    boolean toString;

    public Employee(String id, String name, int age, double salary) {
        this.id = id;
        this.name = name;
        this.age = age;
        this.salary = salary;
    }

    public String getId() {
        return id;
    }

    public String getName() {
        return name;
    }

    public int getAge() {
        return age;
    }

    public double getSalary() {
        return salary;
    }

    @Override
    public String toString() {
        return "\nEmployee{" + "id=" + id + ", name=" + name + ", age=" + age + ", salary=" + salary + '}';
    }
}
